RewriteRule ^kiem-tra-tien/khoang-ngay/([0-9]+)/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/$ kiemtra_tien.php?staff=$1&kieu=2&ngay_start=$2&ngay_end=$3 [L]
RewriteRule ^kiem-tra-tien/khoang-seri/([0-9]+)/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/$ kiemtra_tien.php?staff=$1&kieu=3&seri_start=$2&seri_end=$3 [L]
RewriteRule ^ho-so/$ hoso.php [L]
RewriteRule ^thuong/$ thuong.php [L]
RewriteRule ^thuong/([0-9]+)/$ thuong.php?hsID=$1 [L]
RewriteRule ^phat/$ phat.php [L]
RewriteRule ^phat/([0-9]+)/$ phat.php?hsID=$1 [L]
RewriteRule ^thong-bao/$ thongbao.php [L]
RewriteRule ^level-thuong/$ level_thuong.php [L]
RewriteRule ^level-thuong/([0-9]+)/$ level_thuong.php?hsID=$1 [L]
RewriteRule ^avata-upload/$ upload.php [L]
RewriteRule ^dang-nhap/$ sign_in.php [L]
RewriteRule ^dang-xuat/$ logout.php [L]

RewriteRule ^xuly-tongquan/$ ajax/xuly_tongquan.php [L]
RewriteRule ^xuly-trogiang/$ ajax/xuly_trogiang.php [L]
