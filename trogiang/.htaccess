Options -Indexes

RewriteEngine on

RewriteRule ^tong-quan/$ tongquan.php [L]
RewriteRule ^hoc-sinh-chi-tiet/$ hocsinh_detail.php [L]
RewriteRule ^diem-danh-hoc-sinh/$ diemdanh_hocsinh.php [L]
RewriteRule ^hoc-sinh-chi-tiet/([a-zA-Z0-9_-]+)/$ hocsinh_detail.php?ngay=$1 [L]
RewriteRule ^hoc-sinh-chi-tiet/ma/([a-zA-Z0-9_-]+)/$ hocsinh_detail.php?maso=$1 [L]
RewriteRule ^lich-lam/$ tq_lich.php [L]
RewriteRule ^quan-ly-tien/$ quanly_tien.php [L]
